{
    "name": "beefy-databarn",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "clean": "nx run-many --target=clean --all && rm -rf dist",
        "build": "nx run-many --target=build --all",
        "lint": "nx run-many --target=lint --all",
        "format": "nx run-many --target=format --all && yarn --silent format:top-level",
        "format:top-level": "./node_modules/.bin/prettier . '!packages/**/*' --write --log-level silent",
        "test": "nx run-many --target=test --all",
        "affected:build": "nx affected -t build --parallel",
        "affected:test": "nx affected -t test --parallel",
        "affected:lint": "nx affected -t lint --parallel",
        "affected:format": "nx affected -t format --parallel",
        "precommit": "yarn --silent format:top-level && nx affected -t format lint build test --parallel"
    },
    "private": true,
    "dependencies": {
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@nx/jest": "^17.2.4",
        "@nx/js": "17.2.2",
        "@types/jest": "^29.5.11",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.10.4",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "nx": "17.2.2",
        "prettier": "^3.1.1",
        "prettier-plugin-organize-imports": "^3.2.4",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "workspaces": [
        "packages/*"
    ]
}
